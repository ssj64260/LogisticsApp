<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tl="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.msqsoft.aboutapp.ui.MainActivity">

    <FrameLayout
        android:id="@+id/fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_D1D1D1" />

    <com.flyco.tablayout.CommonTabLayout
        android:id="@+id/com_tablayout"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="@color/color_FBFBFB"
        tl:tl_iconHeight="28dp"
        tl:tl_iconWidth="28dp"
        tl:tl_indicator_color="@color/color_181818"
        tl:tl_indicator_height="0dp"
        tl:tl_textSelectColor="@color/color_181818"
        tl:tl_textUnselectColor="@color/color_181818"
        tl:tl_textsize="@dimen/textsize_12"
        tl:tl_underline_color="@color/color_D1D1D1"
        tl:tl_underline_height="0dp" />

</LinearLayout>
